<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title> 
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" type="text/css">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.min.js"></script>
</head>
<body>
    <nav class="navbar navbar-expand-sm bg-dark navbar-dark">
        <div class="container-fluid" id="container">
          <ul class="navbar-nav">
            <% if(typeof nav=='object' && nav){ nav.forEach(function(Mystic){ %>
              <li class="nav-item">
                <a class="nav-link" href="/mystics/<%=Mystic.nick%>">
                  <%=Mystic.title%>
                </a>
              </li>
              <% })} %>
          </ul>
        </div>
      </nav>
  
  <%- body %>
</body>
<% if(typeof nav == 'object' && nav){ nav.forEach(function(mystic){ %>
  <li class="nav-item">
  <a class="nav-link" href="/mystics/<%=mystic.nick%>"><%=mystic.title%></a>
  </li>
  <% })} %>
  

</html>
